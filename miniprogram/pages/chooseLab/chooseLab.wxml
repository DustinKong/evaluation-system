<cu-custom bgColor="bg-gradual-blue" isBack="{{true}}">
  <view slot="backText">返回</view>
  <view slot="content">选择实验室</view>
</cu-custom>
<view wx:if="{{type=='2'}}" class="cu-list menu card-menu margin-top" >
  <view  class="cu-item arrow" bindtap="goSum" data-id="{{item.id}}">
      <view class="content">
        <text class="cuIcon-circlefill text-grey"></text>
        <text class="text-red text-bold">实验室综合评估</text>
      </view>
    </view>
</view>


<view class="cu-list menu card-menu margin-top" wx:for="{{Lab}}">
  <view class="cu-item arrow" bindtap="go" data-id="{{item.id}}">
      <view class="content">
        <text class="cuIcon-circlefill text-grey"></text>
        <text class="text-black">{{item.title}}</text>
      </view>
    </view>
</view>
